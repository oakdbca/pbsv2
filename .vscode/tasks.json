{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "node: serve",
            "path": "/govapp/frontend/pbs",
            "options": {
                "env": {
                    "PORT": "9152"
                }
            },
            "type": "npm",
            "script": "serve",
            "isBackground": true,
            "problemMatcher": [
                {
                    "base": "$tsc-watch",
                    "background": {
                        "activeOnStart": true,
                        "beginsPattern": "Starting development server",
                        "endsPattern": "Compiled successfully"
                    }
                }
            ],
        }
    ]
}